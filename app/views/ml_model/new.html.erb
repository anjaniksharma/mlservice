
<h1>Models</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:mlmodel, url: new_path) do |f| %>
      
      <%= f.label "Input Text" %>
      <%= f.text_field :inputtext, class: 'control-label' %>
      <%= f.label "Models" %>
      <%= f.select :mdls, MlModel.all.pluck(:name,:id), {}, {class: "form-control"} %>
     
      <%= f.submit "Predict", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

