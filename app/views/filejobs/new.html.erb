<% provide(:title, 'FileJobs') %>
<h1>Create Classification job</h1>



<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <%= form_for(@filejob, html: { multipart: true }) do |f| %>
    <%= render 'shared/error_messages' , object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :modelname %>
      <%= f.select :modelname, MlModel.all.pluck(:name,:name), {}, {class: "form-control"} %>

      <%= f.label :inputfile %>
      <%= f.file_field :inputfile %>
      
     
      <%= f.submit "Upload File Job", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>